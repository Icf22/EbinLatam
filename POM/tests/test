import { test, expect } from '@playwright/test';

test('test', async ({ page }) => {
  await page.goto('https://ebinduat-services-latam.egl-cloud.com/');
  await page.getByRole('button', { name: 'Ir a inicio de sesión' }).click();
  await page.getByPlaceholder('Nombre de usuario').click();
  await page.getByPlaceholder('Nombre de usuario').press('Control+V');
  await page.getByPlaceholder('Contraseña').click();
  await page.getByPlaceholder('Contraseña').press('Control+V');
  await page.getByRole('button', { name: 'Ingresar' }).click();
  await page.getByText('more_horiz').click();
  await page.getByText('more_horiz').click();
  await page.getByText('more_horiz').click();
  await page.getByText('more_horiz').click();
  await page.getByText('more_horiz').click();
  await page.getByText('arrow_drop_down').nth(1).click();
  await page.getByText('BBVA Perú').click();
  await page.getByText('arrow_drop_down').nth(1).click();
  await page.locator('label').filter({ hasText: 'Tipo de fecha*arrow_drop_down' }).locator('i').click();
  await page.locator('label').filter({ hasText: 'Tipo de fecha*arrow_drop_down' }).locator('i').click();
  await page.getByLabel('Tipo de fecha*').click();
  await page.locator('label').filter({ hasText: 'Tipo de fecha*arrow_drop_down' }).locator('i').click();
  await page.getByText('arrow_drop_down').nth(1).click();
  await page.getByRole('listbox').getByText('BBVA Perú').click();
  await page.getByLabel('Fecha inicial*').click();
  await page.getByLabel('Fecha inicial*').fill('2023/06/01');
  await page.getByLabel('Fecha final*').click();
  await page.getByLabel('Fecha final*').fill('2023/07/17');
  await page.locator('div').filter({ hasText: /^Tipo de fecha\*errorarrow_drop_downPor favor seleccione una opción válida$/ }).first().click();
  await page.getByText('Fecha Proceso', { exact: true }).click();
  await page.getByText('arrow_drop_down').nth(2).click();
  await page.getByRole('listbox').getByText('Fecha Proceso').click();
  await page.getByRole('button', { name: 'Consultar' }).click();
  await page.getByText('16706').click();
  await page.locator('label').filter({ hasText: '10arrow_drop_down' }).locator('i').click();
  await page.locator('button:nth-child(5)').click();
  await page.locator('button:nth-child(5)').click();
  await page.locator('.q-table__bottom > div > button:nth-child(3)').click();
  await page.locator('.q-table__bottom > div > button:nth-child(3)').click();
  await page.locator('.q-table__bottom > div > button:nth-child(3)').click();
  await page.locator('.q-table__bottom > div > button:nth-child(3)').click();
});